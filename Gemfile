source 'https://rubygems.org'

gem 'jekyll', '3.9.1'
# Use thin for videos to work
gem 'thin'
gem 'kramdown', '2.3.1'
# The kramdown GFM parser was only part of the kramdown distribution until kramdown version 2.0.0.
gem 'kramdown-parser-gfm'

group :jekyll_plugins do
  gem 'jekyll-paginate'
  gem 'jekyll-sitemap'
end

gem "webrick", "~> 1.7"
